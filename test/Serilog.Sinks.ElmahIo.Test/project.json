{
  "version": "1.0.0-*",
  "tools": {
    "dotnet-version": "1.1.0"
  },
  "dependencies": {
    "dotnet-test-nunit": "3.4.0-beta-3",
    "Moq": "4.7.0",
    "NUnit": "3.6.0",
    "Serilog.Sinks.ElmahIo": { "target": "project" }
  },
  "testRunner": "nunit",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [
          "DOTNETCORE"
        ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      },
      "imports": [ "dnxcore50", "portable-net45+win8" ]
    },
    "net451": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1"
      }
    }
  }
}